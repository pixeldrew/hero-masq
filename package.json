{
  "name": "hero-masq",
  "version": "1.0.0-alpha.1",
  "description": "dnsmasq config manager",
  "main": "index.js",
  "license": "MIT",
  "repository": "https://github.com/pixeldrew/hero-masq.git",
  "scripts": {
    "dev": "nodemon server.js",
    "build": "next build",
    "start": "NODE_ENV=production node server.js",
    "test": "jest",
    "clean": "rimraf .next/"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint ./ --fix",
      "pretty-quick --staged",
      "git add"
    ]
  },
  "nodemonConfig": {
    "watch": [
      "server.js",
      "routes.js",
      "next.config.js",
      "server",
      "lib/constants.js"
    ],
    "delay": "2500"
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.0.1",
    "@apollo/react-ssr": "^3.0.1",
    "@material-ui/core": "^4.4.0",
    "@material-ui/icons": "^4.2.1",
    "apollo-boost": "^0.4.4",
    "apollo-cache-inmemory": "^1.6.3",
    "apollo-client": "^2.6.4",
    "apollo-link": "^1.2.12",
    "apollo-link-context": "^1.0.18",
    "apollo-link-error": "^1.1.11",
    "apollo-link-http": "^1.5.15",
    "apollo-link-ws": "^1.0.18",
    "apollo-server-express": "^2.9.3",
    "apollo-utilities": "^1.3.2",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.4",
    "date-fns": "^2.0.1",
    "dnsmasq-leases": "^0.1.2",
    "dotenv-defaults": "^1.0.2",
    "express": "^4.17.1",
    "express-jwt": "^5.3.1",
    "express-unless": "^0.5.0",
    "fs-extra": "^8.1.0",
    "graphql": "^14.5.4",
    "graphql-directive-uid": "^0.2.0",
    "graphql-import": "^0.7.1",
    "graphql-iso-date": "^3.6.1",
    "graphql-subscriptions": "^1.1.0",
    "graphql-tag": "^2.10.1",
    "graphql-tools": "^4.0.5",
    "graphql-validated-types": "^2.6.0",
    "helmet": "^3.20.1",
    "isomorphic-unfetch": "^3.0.0",
    "js-cookie": "^2.2.1",
    "jsonwebtoken": "^8.5.1",
    "netmask": "^1.0.6",
    "next": "^9.0.5",
    "next-offline": "^4.0.5",
    "next-routes": "^1.4.2",
    "qs-stringify": "^1.1.0",
    "react": "^16.9.0",
    "react-apollo": "^2.5.8",
    "react-dom": "^16.9.0",
    "react-text-mask": "^5.4.3",
    "subscriptions-transport-ws": "^0.9.16",
    "yup": "^0.27.0"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-optional-chaining": "^7.6.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint": "^6.3.0",
    "eslint-config-prettier": "^6.2.0",
    "eslint-loader": "^3.0.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "husky": "^3.0.5",
    "jest": "^24.9.0",
    "lint-staged": "^9.2.5",
    "nodemon": "^1.19.2",
    "prettier": "1.18.2",
    "pretty-quick": "^1.11.1",
    "prop-types": "^15.7.2",
    "react-test-renderer": "^16.9.0",
    "rimraf": "^3.0.0"
  }
}
